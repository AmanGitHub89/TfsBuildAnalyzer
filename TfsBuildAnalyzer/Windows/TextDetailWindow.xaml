<Window x:Class="TfsBuildAnalyzer.Windows.TextDetailWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Detail" Height="600" Width="1000">
    <Grid Background="DarkGray">
        <ScrollViewer Margin="10,10,10,10" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Background="LightGray">
            <TextBox Name="TextBox1" Padding="10,20,10,20" IsReadOnly="True" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Background="LightGray"></TextBox>
        </ScrollViewer>
    </Grid>
</Window>
